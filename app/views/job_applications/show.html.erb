<div class="page-header">
  <%= link_to job_applications_path, :class => 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_job_application_path(@job_application), :class => 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show job_application</h1>
</div>

<dl class="dl-horizontal">
  <dt>Wage:</dt>
  <dd><%= @job_application.wage %></dd>

  <dt>Note:</dt>
  <dd><%= @job_application.note %></dd>

  <dt>Applicant:</dt>
  <dd><%= @job_application.applicant_id %></dd>

  <dt>Advertisement:</dt>
  <dd><%= @job_application.advertisement_id %></dd>

</dl>
